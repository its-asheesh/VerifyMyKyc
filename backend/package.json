{
  "name": "verifymykyc-backend",
  "version": "1.0.0",
  "description": "Backend API for VerifyMyKyc",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --exec ts-node server.ts",
    "build": "tsc",
    "prod": "node server.js",
    "seed-pricing": "ts-node src/modules/pricing/seed-pricing.ts",
    "seed-users": "ts-node src/modules/auth/seed-users.ts",
    "seed-orders": "ts-node src/modules/orders/seed-orders.ts",
    "seed-carousel": "ts-node src/modules/carousel/seed-carousel.ts",
    "seed-locations": "ts-node src/modules/auth/seed-users-location.ts",
    "seed-blogs": "ts-node src/modules/blog/seed-blogs.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@types/exceljs": "^0.5.3",
    "@types/nodemailer": "^7.0.1",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "firebase-admin": "^12.7.0",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "razorpay": "^2.9.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "winston": "^3.19.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.13",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "nodemon": "^3.0.1",
    "prettier": "^3.8.1",
    "typescript-eslint": "^8.54.0"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts,json",
    "ignore": [
      "src/**/*.spec.ts",
      "node_modules"
    ],
    "exec": "ts-node server.ts"
  }
}